{
  "extends": "astro/tsconfigs/strict",
  "include": ["src/**/*.d.ts", "src/**/*.ts", "src/**/*.astro"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/components/*": ["src/components/*"],
      "@/layouts/*": ["src/layouts/*"],
      "@/pages/*": ["src/pages/*"],
      "@/css/*": ["src/css/*"]
    },
    "jsx": "preserve"
  }
}
